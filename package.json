{
  "name": "hushh-technologies",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:vercel": "vercel dev --listen 3000",
    "build": "vite build && node scripts/generate-sitemap.js && node scripts/generate-robots.js",
    "preview": "vite preview",
    "ios:sync": "npx cap sync ios",
    "ios:open": "npx cap open ios",
    "ios:build": "npm run build && npx cap sync ios",
    "ios:archive": "npm run ios:build && cd ios/App && xcodebuild -workspace App.xcworkspace -scheme App -configuration Release -archivePath build/App.xcarchive archive",
    "ios:export": "cd ios/App && xcodebuild -exportArchive -archivePath build/App.xcarchive -exportPath build/export -exportOptionsPlist ../ExportOptions.plist",
    "ios:deploy": "./scripts/ios-deploy.sh",
    "ios:deploy:build-only": "./scripts/ios-deploy.sh --no-upload",
    "ios:upload": "xcrun altool --upload-app -f ios/App/build/export/App.ipa -t ios --apiKey $APP_STORE_API_KEY_ID --apiIssuer $APP_STORE_ISSUER_ID",
    "ios:testflight": "./scripts/ios-testflight.sh",
    "android:sync": "npx cap sync android",
    "android:open": "npx cap open android",
    "android:build": "npm run build && npx cap sync android",
    "android:playstore": "./scripts/android-playstore.sh",
    "android:debug": "cd android && ./gradlew assembleDebug",
    "android:release": "cd android && ./gradlew bundleRelease",
    "test:e2e": "npx playwright test",
    "test:e2e:deepsearch": "npx playwright test deepsearch",
    "test:e2e:ui": "npx playwright test --ui",
    "test:e2e:headed": "npx playwright test --headed",
    "test:e2e:report": "npx playwright show-report",
    "test:e2e:debug": "npx playwright test --debug"
  },
  "dependencies": {
