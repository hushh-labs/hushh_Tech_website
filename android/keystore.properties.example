# Android Keystore Configuration for Release Builds
# Copy this file to keystore.properties and fill in the values
# IMPORTANT: Never commit keystore.properties to version control!

# Path to your keystore file (relative to android/ directory)
storeFile=keystores/hushh-release.keystore

# Keystore password
storePassword=YOUR_KEYSTORE_PASSWORD

# Key alias (name of the key in the keystore)
keyAlias=hushh-release

# Key password
keyPassword=YOUR_KEY_PASSWORD
