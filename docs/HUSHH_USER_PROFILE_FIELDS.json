{
  "title": "Hushh User Profile - Complete Field Documentation",
  "description": "Documentation of all fields that appear after slug generation and AI generation in the hushh-user-profile page",
  "last_updated": "2025-12-20",
  
  "profile_sections": {
    
    "1_basic_info": {
      "section_name": "Basic Information",
      "description": "User provided fields (form input)",
      "source": "user_input",
      "fields": {
        "name": {
          "label": "Full Name",
          "type": "string",
          "required": true,
          "editable": true,
          "source": "user_input"
        },
        "email": {
          "label": "Email",
          "type": "string",
          "required": true,
          "editable": true,
          "source": "user_input"
        },
        "age": {
          "label": "Age",
          "type": "number",
          "required": true,
          "editable": true,
          "source": "user_input OR calculated_from_date_of_birth"
        },
        "phoneCountryCode": {
          "label": "Country Code",
          "type": "string",
          "required": true,
          "editable": true,
          "source": "user_input",
          "example": "+1"
        },
        "phoneNumber": {
          "label": "Phone Number",
          "type": "string",
          "required": true,
          "editable": true,
          "source": "user_input"
        },
        "organisation": {
          "label": "Organisation",
          "type": "string",
          "required": false,
          "editable": true,
          "source": "user_input"
        }
      }
    },
    
    "2_generated_slug": {
      "section_name": "Generated Slug & Profile URL",
      "description": "Auto-generated unique identifier for public profile URL",
      "source": "auto_generated",
      "fields": {
        "slug": {
          "label": "Profile Slug",
          "type": "string",
          "editable": false,
          "source": "auto_generated_by_supabase",
          "example": "john-doe-abc123"
        },
        "profile_url": {
          "label": "Public Profile URL",
          "type": "string",
          "editable": false,
          "source": "derived_from_slug",
          "format": "https://hushhtech.com/investor/{slug}",
          "example": "https://hushhtech.com/investor/john-doe-abc123"
        }
      }
    },
    
    "3_ai_generated_investor_profile": {
